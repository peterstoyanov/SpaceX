<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Space X</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/magnific-popup.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/fontawesome-all.min.css" />
    <link rel="stylesheet" href="~/slick/slick.css" />
    <link rel="stylesheet" href="~/slick/slick-theme.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/tooplate-style.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400">
    <script src="~/js/renderPage.js"></script>
</head>
<body>
    <!-- Loader -->
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>

    <!-- Page Content -->
    <div class="container-fluid tm-main">
        <div class="row tm-main-row">

            <!-- Sidebar -->
            <div id="tmSideBar" class="col-xl-3 col-lg-4 col-md-12 col-sm-12 sidebar">

                <button id="tmMainNavToggle" class="menu-icon">&#9776;</button>

                <div class="inner">
                    <nav id="tmMainNav" class="tm-main-nav">
                        <ul>
                            <li>
                                <a href="#intro" id="tmNavLink1" class="scrolly active" data-bg-img="1.jpg" data-page="#tm-section-1">
                                    <i class="fas fa-home tm-nav-fa-icon"></i>
                                    <span>Introduction</span>
                                </a>
                            </li>
                            <li>
                                <a href="#products" id="tmNavLink2" class="scrolly" data-bg-img="2.jpg" data-page="#tm-section-2" data-page-type="carousel">
                                    <i class="fas fa-map tm-nav-fa-icon"></i>
                                    <span>Mission Patches</span>
                                </a>
                            </li>
                            <li>
                                <a href="#company" class="scrolly" data-bg-img="3.jpg" data-page="#tm-section-3">
                                    <i class="fas fa-users tm-nav-fa-icon"></i>
                                    <span>Launch plans</span>
                                </a>
                            </li>
                            <li>
                                <a href="#contact" class="scrolly" data-bg-img="4.jpg" data-page="#tm-section-4">
                                    <i class="fas fa-comments tm-nav-fa-icon"></i>
                                    <span>Contact me</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>

            <div class="col-xl-9 col-lg-8 col-md-12 col-sm-12 tm-content">

                <div class="container">
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>
            </div>	<!-- .tm-content -->
            <footer class="footer-link">
                <p class="tm-copyright-text">
                    SpaceX API
                </p>
            </footer>
        </div>	<!-- row -->
    </div>

    <div id="preload-01"></div>
    <div id="preload-02"></div>
    <div id="preload-03"></div>
    <div id="preload-04"></div>

    <!-- JS -->
    <script src="~/lib/jquery/dist/jquery-3.2.1.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.magnific-popup.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.backstretch.min.js"></script>
    <script src="~/slick/slick.min.js"></script> <!-- Slick Carousel -->
    <script src="~/js/site.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
